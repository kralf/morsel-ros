    self.libraries = "${ROS_ROSCPP_LIBRARIES};${ROS_TF_LIBRARIES}".split(";")
    self.libraryDirs = "${ROS_ROSCPP_LIBRARY_DIRS};${ROS_TF_LIBRARY_DIRS}".split(";")
